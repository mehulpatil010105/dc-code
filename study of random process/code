
clc;
clear;
close;
count=csvRead('C:\scilab projects\user defined functions\count.dat');
for i=1:3
    scf();
    mu(i)=mean(count(:,i));
    sigma(i)=stdev(count(:,i));
    histplot(24,count(:,i));
    xtitle('roll no=E3160 Histrogram of traffic intersection'+string(i));
end
MeanTotal=mean(mean(count));
disp('Mean for individual Traffic intersection is=',mu);
disp('Standard Deviation for individual Trafffic intersection is=',sigma);
disp('Overall Mean=',MeanTotal);


